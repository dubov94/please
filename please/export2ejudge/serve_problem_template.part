[problem]
id = $id$
super = "Generic"
short_name = "$shortname$"
long_name = "$longname$"
internal_name = "$internalname$"
!(problem_config['input'] != 'stdin')!input_file = "$inputfile$"
!(problem_config['output'] != 'stdout')!output_file = "$outputfile$"
!(problem_config['input'] == 'stdin')!use_stdin
!(problem_config['output'] == 'stdout')!use_stdout
time_limit_millis = $timelimit_ms$
test_pat = "%02d"
corr_pat = "%02d.a"
max_vm_size = $memorylimit_kb$K
check_cmd = "$checker$"
